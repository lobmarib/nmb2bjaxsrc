            <complexType>
              <choice>
                <element ref="adr:AbstractArgumentInErrorStringConditionExtension"/>
              </choice>
              <attributeGroup ref="gml:OwnershipAttributeGroup"/>
            </complexType>
          </element>
        </sequence>
      </extension>
    </complexContent>
