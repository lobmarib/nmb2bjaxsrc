        <annotation>
          <documentation/>
        </annotation>
      </element>
      <choice maxOccurs="unbounded" minOccurs="0">
        <annotation>
          <documentation>Reference Choice</documentation>
        </annotation>
        <element minOccurs="0" name="reference_airportHeliport" nillable="true" type="aixm:AirportHeliportPropertyType"/>
        <element minOccurs="0" name="reference_airspace" nillable="true" type="aixm:AirspacePropertyType"/>
        <choice>
          <annotation>
            <documentation>A link class that allows selecting between a navaid system, a runway point, an airport reference point, an aiming point or a fix designated point.&#13;
SignificantPoint accounts for a specified geographical location used to define an ATS route, the flight path of an aircraft or for other navigation/ATS purposes.</documentation>
          </annotation>
          <element minOccurs="0" name="reference_significantPoint_fixDesignatedPoint" nillable="true" type="aixm:DesignatedPointPropertyType"/>
          <element minOccurs="0" name="reference_significantPoint_navaidSystem" nillable="true" type="aixm:NavaidPropertyType"/>
